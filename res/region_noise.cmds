   mode mul
   fill 1.3

   show_region_sectors
   end

   # most range: 0.2 - 0.8 => push 
   map_range 0.5   1    0.6  1.005
   map_range 0     0.4  0    0.3
   map_range 0.4   0.5  0.3  0.6
   show_region_sectors
   end

   map_range 0.28 0.75 -1 -1
   map_range 0 0.28 0.75 0.5
   end

src_dst 1 2
   fill_noise 4 2 0.4 1
   map_range 0 1 0 0.499999

   # now blend in first noise:
   src_range 0.5 1
   src_blend 1
   fill

