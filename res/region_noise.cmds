src_dst 0 0
   fill 0.01

src_dst 0 1
   fill_noise 3 3 0.4
   hist_equalize 512 0 1
   show_region_sectors
 #  show_range_region_sector E4
   end

   dst_range 0 1

 #  dst_range 0.3 0.65
